# regfont <https://github.com/dcpurton/regfont>

complete -c regfont -s a -l add -r -d 'Add specified fonts'
complete -c regfont -s r -l remove -r -d 'Remove specified fonts'
complete -c regfont -s h -l help -d 'Print help message'
complete -c regfont -s v -l version -d 'Print version information'
complete -c regfont -s d -l debug -d 'Turn on debugging information'
