{
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "code-runner.clearPreviousOutput": true,
  "code-runner.customCommand": "echo $PATH",
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "fish": "fish -C 'set -gx TERM dumb'",
    "html": "links -dump",
    "jq": "jq --null-input --from-file",
    "markdown": "multimarkdown -s --nolabels",
    "ruby": "ruby -w",
    "sass": "/usr/local/bin/sass --no-source-map -C -t expanded --trace",
    "scss": "/usr/local/bin/scss --no-source-map -C -t expanded --trace",
    "slim": "slimrb --option format=:html --option tabsize=2 --pretty --trace",
    "troff": "man -Pcat"
  },
  "code-runner.executorMapByGlob": {
    "*.[1-9].md": "ronn --pipe < $fullFileName | man -Pcat -Eutf8 -l -",
    "*.ti": "tic -Lxfq $fullFileName"
  },
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": false,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "code-runner.temporaryFileName": "",
  "debug.showInlineBreakpointCandidates": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.colorDecorators": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorStyle": "line-thin",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.seedSearchStringFromSelection": false,
  "editor.foldingHighlight": false,
  "editor.fontFamily": "IBM Plex Mono, Office Code Pro, Consolas, monospace",
  "editor.fontSize": 12,
  "editor.fontWeight": "300",
  "editor.formatOnSaveMode": "modifications",
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 120,
  "editor.lightbulb.enabled": false,
  "editor.links": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    80,
    118
  ],
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showSnippets": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 118,
  "emojisense.emojiDecoratorsEnabled": false,
  "emojisense.markupCompletionsEnabled": false,
  "extensions.closeExtensionDetailsOnViewChange": false,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.[1-9]": "troff",
    "*.ignore": "ignore",
    "*.make": "makefile",
    "*.plist": "xml",
    "*.sublime-syntax": "yaml",
    "*.sublime-{settings,build,keymap}": "json",
    "*.vifm": "viml",
    "ignore": "ignore",
    "vimrc": "viml"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.eol": "\n",
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.defaultCloneDirectory": "/Users/zozo/.local/src/github",
  "git.openDiffOnClick": false,
  "git.path": "/usr/local/bin/git",
  "gitlens.blame.avatars": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD @ HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.menus": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.statusBar.format": "${author}, ${agoOrDate} · ${message}",
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 0,
  "insertGuid.showCodeSnippets": false,
  "jqPlayground.binaryPath": "/usr/local/bin/jq",
  "json.maxItemsComputed": 8192,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.extension.syntax.decorations": false,
  "markdown.extension.toc.levels": "1..3",
  "markdown.extension.toc.orderedList": true,
  "markdownShortcuts.icons.bold": false,
  "markdownShortcuts.icons.bullets": false,
  "markdownShortcuts.icons.citations": false,
  "markdownShortcuts.icons.image": false,
  "markdownShortcuts.icons.italic": false,
  "markdownShortcuts.icons.link": false,
  "markdownShortcuts.icons.strikethrough": false,
  "markdownlint.config": {
    "MD007": {
      "indent": 4
    },
    "MD009": {
      "strict": true
    },
    "MD010": {
      "code_blocks": false
    },
    "MD013": false,
    "MD024": {
      "siblings_only": true
    },
    "MD025": false,
    "MD026": {
      "punctuation": ",;"
    },
    "MD028": false,
    "MD032": false,
    "MD033": false,
    "MD036": false,
    "MD040": false,
    "MD041": false,
    "MD043": false,
    "MD044": false,
    "MD045": false
  },
  "outline.problems.enabled": false,
  "ruby.interpreter.commandPath": "/Users/zozo/.rbenv/shims/ruby",
  "ruby.pathToBundler": "/Users/zozo/.rbenv/shims/bundle",
  "ruby.useLanguageServer": false,
  "search.searchEditor.defaultNumberOfContextLines": 2,
  "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "settingsSync.ignoredExtensions": [
    "castwide.solargraph"
  ],
  "settingsSync.ignoredSettings": [
    "code-runner.customCommand",
    "code-runner.executorMap",
    "code-runner.executorMapByGlob",
    "code-runner.terminalRoot",
    "editor.fontFamily",
    "editor.fontSize",
    "editor.fontWeight",
    "git.defaultCloneDirectory",
    "http.proxy",
    "http.proxyAuthorization",
    "http.proxyStrictSSL",
    "jqPlayground.binaryPath",
    "shellcheck.executablePath",
    "solargraph.bundlerPath",
    "solargraph.commandPath",
    "spellright.language",
    "terminal.external.osxExec",
    "terminal.integrated.automationShell.osx",
    "terminal.integrated.env.osx",
    "terminal.integrated.shell.osx",
    "terminal.integrated.shellArgs.osx",
    "window.doubleClickIconToClose"
  ],
  "settingsSync.keybindingsPerPlatform": false,
  "shellcheck.enableQuickFix": true,
  "shellcheck.exclude": [
    "1090",
    "1091"
  ],
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "shellcheck.run": "onSave",
  "shellcheck.useWorkspaceRootAsCwd": true,
  "solargraph.bundlerPath": "/Users/zozo/.rbenv/shims/bundle",
  "solargraph.commandPath": "/Users/zozo/.rbenv/shims/solargraph",
  "solargraph.completion": true,
  "solargraph.definitions": true,
  "solargraph.diagnostics": true,
  "solargraph.folding": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "solargraph.references": true,
  "solargraph.rename": true,
  "solargraph.symbols": true,
  "sortJSON.contextMenu": {
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONTypeReverse": false,
    "sortJSONValuesReverse": false
  },
  "sortJSON.orderUnderride": [
    "[fish]",
    "[makefile]",
    "[markdown]",
    "[plaintext]",
    "[ruby]",
    "[yaml]"
  ],
  "spellright.addToSystemDictionary": true,
  "spellright.configurationUpdate": false,
  "spellright.documentTypes": [
    "markdown",
    "git-commit"
  ],
  "spellright.groupDictionaries": false,
  "spellright.language": [
    "en_CA"
  ],
  "spellright.recheckOnSave": false,
  "spellright.suggestionsInHints": false,
  "synthwave84.disableGlow": true,
  "task.saveBeforeRun": "prompt",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.automationShell.osx": "/usr/local/bin/bash",
  "terminal.integrated.env.osx": {
    "PREFERRED_SHELL": "bash"
  },
  "terminal.integrated.env.windows": {
    "PREFERRED_SHELL": "bash"
  },
  "terminal.integrated.scrollback": 10240,
  "terminal.integrated.shell.windows": "Z:\\bin\\bash.exe",
  "window.closeWhenEmpty": true,
  "window.newWindowDimensions": "offset",
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.title": "${dirty}${activeEditorShort}${separator}${activeFolderMedium}${separator}${rootName}",
  "window.titleSeparator": " · ",
  "workbench.activityBar.iconClickBehavior": "focus",
  "workbench.colorTheme": "Monokai Pro (Filter Machine)",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.untitled.labelFormat": "name",
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "xmlTools.enableXmlTreeViewCursorSync": true,
  "xmlTools.enableXmlTreeViewMetadata": false,
  "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
  "[fish]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
    },
    "editor.formatOnPaste": false,
    "editor.quickSuggestions": {
      "other": false
    },
    "editor.wordWrap": "bounded"
  },
  "[plaintext]": {
    "editor.wordBasedSuggestions": false
  },
  "[ruby]": {
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.wordWrapColumn": 80
  }
}
