{
  "bracket-pair-colorizer-2.colors": [
    "#b58900",
    "#6c71c4",
    "#268bd2"
  ],
  "bracket-pair-colorizer-2.unmatchedScopeColor": "#dc322f",
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "code-runner.clearPreviousOutput": true,
  "code-runner.customCommand": "echo $PATH",
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "fish": "fish -C 'set -gx TERM dumb'",
    "html": "links -dump",
    "markdown": "multimarkdown -s --nolabels",
    "ruby": "ruby -w",
    "sass": "/usr/local/bin/sass --no-source-map -C -t expanded --trace",
    "scss": "/usr/local/bin/scss --no-source-map -C -t expanded --trace",
    "slim": "slimrb --option format=:html --option tabsize=2 --pretty --trace",
    "troff": "man -Pcat"
  },
  "code-runner.executorMapByGlob": {
    "*.[1-9].md": "ronn --pipe < $fullFileName | man -Pcat -Eutf8 -l -",
    "*.ti": "tic -Lxfq $fullFileName"
  },
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": false,
  "code-runner.temporaryFileName": "",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorStyle": "line-thin",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.autoFindInSelection": "multiline",
  "editor.find.seedSearchStringFromSelection": false,
  "editor.fontFamily": "Native-Light, IBMPlexMono-Light, Office Code Pro, Consolas, monospace",
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 120,
  "editor.lightbulb.enabled": false,
  "editor.links": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    80,
    118
  ],
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.filterGraceful": false,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 118,
  "emojisense.emojiDecoratorsEnabled": false,
  "explorer.confirmDelete": false,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.[1-9]": "troff",
    "*.ignore": "ignore",
    "*.make": "makefile",
    "*.plist": "xml",
    "*.sublime-{settings,build,keymap}": "json",
    "*.sublime-syntax": "yaml",
    "*.vifm": "viml",
    "ignore": "ignore",
    "vimrc": "viml"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD @ HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.statusBar.format": "${author}, ${agoOrDate} Â· ${message}",
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 0,
  "insertGuid.showCodeSnippets": false,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.list.indentationSize": "adaptive",
  "markdown.extension.syntax.decorations": false,
  "markdownlint.config": {
    "MD007": { "indent": 2 },
    "MD009": { "strict": true },
    "MD010": { "code_blocks": false },
    "MD013": false,
    "MD024": { "siblings_only": true },
    "MD025": false,
    "MD026": { "punctuation": ",;" },
    "MD028": false,
    "MD032": false,
    "MD033": false,
    "MD036": false,
    "MD040": false,
    "MD041": false,
    "MD043": false,
    "MD044": false,
    "MD045": false
  },
  "markdownlint.run": "onSave",
  "material-icon-theme.files.associations": {
    "config": "settings"
  },
  "material-icon-theme.folders.associations": {
    "Code": "vscode",
    "environment.d": "environment",
    "etc": "other",
    "git": "git",
    "npm": "node",
    "skel": "template",
    "ssh": "keys"
  },
  "ruby.interpreter.commandPath": "/Users/zozo/.rbenv/shims/ruby",
  "ruby.useLanguageServer": true,
  "scm.defaultViewMode": "tree",
  "search.maintainFileSearchCache": true,
  "search.smartCase": true,
  "shellcheck.enable": false,
  "shellcheck.exclude": [
    1090,
    1091
  ],
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "shellcheck.run": "onSave",
  "shellcheck.useWorkspaceRootAsCwd": true,
  "solargraph.checkGemVersion": false,
  "solargraph.commandPath": "/Users/zozo/.rbenv/shims/solargraph",
  "solargraph.completion": false,
  "solargraph.definitions": false,
  "solargraph.folding": false,
  "solargraph.hover": false,
  "solargraph.references": false,
  "solargraph.rename": false,
  "solargraph.symbols": false,
  "sortJSON.orderUnderride": [
    "[makefile]",
    "[markdown]"
  ],
  "spellright.addToSystemDictionary": false,
  "spellright.configurationScope": "user",
  "spellright.documentTypes": [
    "markdown",
    "git-commit"
  ],
  "spellright.groupDictionaries": false,
  "spellright.language": [
    "en_CA"
  ],
  "spellright.suggestionsInHints": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.automationShell.osx": "/usr/local/bin/bash",
  "terminal.integrated.env.osx": {
    "PREFERRED_SHELL": "bash"
  },
  "terminal.integrated.env.windows": {
    "PREFERRED_SHELL": "bash"
  },
  "terminal.integrated.scrollback": 10240,
  "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  "terminal.integrated.shell.windows": "Z:\\bin\\bash.exe",
  "terminal.integrated.shellArgs.osx": [],
  "window.openFilesInNewWindow": "default",
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Material Theme Palenight",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "xmlTools.enableXmlTreeViewCursorSync": true,
  "xmlTools.enableXmlTreeViewMetadata": false,
  "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
  "[makefile]": {
    "editor.insertSpaces": false
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
    },
    "editor.formatOnPaste": false,
    "editor.quickSuggestions": {
      "other": false
    },
    "editor.wordWrap": "bounded",
    "files.trimTrailingWhitespace": false
  }
}
