{
  "def_inspect": {
    "prefix": "def inspect",
    "body": [
      "# @return [String]",
      "def inspect",
      "  %(#<#{self.class.name}:#{${1:@type}}>)",
      "end"
    ],
    "description": "#inspect"
  },
  "def_initialize": {
    "prefix": "def initialize",
    "body": [
      "def initialize${2:(${1:*args})}",
      "\t$0",
      "end"
    ],
    "description": "#new"
  },
  "frozen_string_literal": {
    "prefix": "frozen",
    "body": [
      "$LINE_COMMENT frozen_string_literal: true\n\n"
    ],
    "description": "`frozen_string_literal` comment"
  },
  "hashbang_ruby": {
    "prefix": "!env",
    "body": [
      "#!/usr/bin/env ruby\n\n"
    ]
  }
}
