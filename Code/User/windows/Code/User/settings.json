{
  "code-runner.customCommand": "echo %PATH%",
  "code-runner.executorMap": {
    "ahk": "AutoHotkeyU64.exe /force /restart /ErrorStdOut=UTF-8",
    "bat": "cmd /c",
    "fish": "fish.exe -C 'set -gx TERM dumb'",
    "html": "links.exe -dump",
    "jq": "jq.exe --null-input --from-file",
    "markdown": "multimarkdown.exe --snippet --nolabels",
    "perl": "perl.exe",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "python": "python.exe",
    "ruby": "ruby.exe -w",
    "sass": "sass.exe --no-source-map --style=expanded --trace",
    "scss": "sass.exe --no-source-map --style=expanded --trace",
    "slim": "slimrb.exe --option format=:html --option tabsize=2 --pretty --trace",
    "troff": "man.exe -Pcat"
  },
  "code-runner.terminalRoot": "/cygdrive/",
  "git.defaultCloneDirectory": "C:\\cygwin64\\home\\zgm\\.local\\src",
  "git.path": "git.exe",
  "jqPlayground.binaryPath": "jq.exe",
  "ruby.interpreter.commandPath": "ruby.exe",
  "ruby.pathToBundler": "bundler.exe",
  "shellcheck.executablePath": "shellcheck.exe",
  "terminal.integrated.shell.windows": "bash.exe",
  "window.doubleClickIconToClose": true,
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "toggle"
}
